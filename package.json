{
  "name": "feathers-hook-validation-jsonschema",
  "description": "Feathers validation layer using JSON Schema.",
  "version": "0.0.0",
  "license": "ISC",
  "keywords": [
    "feathers",
    "feathers-hook",
    "feathers-plugin"
  ],
  "main": "lib",
  "directories": {
    "lib": "lib"
  },
  "standard": {
    "ignore": [
      "lib"
    ]
  },
  "scripts": {
    "compile": "rimraf lib/ && babel --out-dir lib/ src/",
    "lint": "standard --verbose | snazzy",
    "preinstall": "npm prune",
    "prepublish": "npm run compile",
    "pretest": "npm run lint && npm run compile && nsp check",
    "publish": "git push attn && git push attn --tags",
    "release:major": "npm version major && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:patch": "npm version patch && npm publish",
    "release:prerelease": "npm version prerelease && npm publish",
    "test": "echo 'Error: no tests specified' && exit 1",
    "watch": "babel --watch --out-dir lib/ src/"
  },
  "dependencies": {
    "feathers-commons": "^0.7.0",
    "feathers-errors": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "feathers": "^2.0.0",
    "nsp": "^2.2.1",
    "rimraf": "^2.5.2",
    "snazzy": "^3.0.0",
    "standard": "^6.0.8"
  },
  "engines": {
    "node": ">= 0.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/attn/feathers-hook-validation-jsonschema.git"
  },
  "homepage": "https://github.com/attn/feathers-hook-validation-jsonschema",
  "bugs": {
    "url": "https://github.com/attn/feathers-hook-validation-jsonschema/issues"
  },
  "author": {
    "name": "Joshua Jabbour",
    "email": "code@joshuajabbour.com"
  },
  "contributors": []
}
